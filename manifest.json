{
    "manifest_version": 2,
    "name": "Dark Mode & Light Mode",
    "version": "1.0.5",
    "icons":{
        "128": "icon.png"
    },
    "browser_action": {
        "default_title": "LD",
        "default_popup": "interface.html",
        "default_icon": "icon.png"
    },
        "background": {
            "scripts": ["popup.js"],
            "persistent":true
        },
    "permissions": [
        "tabs",
        "https://*/*",
        "http://*/*"
      ]
}